@model Social
@{
    ViewData["Title"] = "Update";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<div style="margin-top:40px;margin-left:30px" class="col-11">
    <div class="bg-light rounded h-100 p-4">
        <div asp-validation-summary="All" class="text-danger"></div>
        <h6 class="mb-4">Social Update</h6>
        <form method="post">
            <div class="mb-3">
                <label asp-for="Name" class="form-label"></label>
                <input asp-for="Name" class="form-control">
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <label asp-for="UpdateDate" class="form-label"></label>
                <input asp-for="UpdateDate" class="form-control">
                <span asp-validation-for="UpdateDate" class="text-danger"></span>
            </div>





            <button type="submit" class="btn btn-primary">Update</button>
        </form>
    </div>
</div>

